    "recipeInstructions": [
        {
            "id": "c0ccea28-b1f5-46cc-b9da-52ef7822422c",
            "ingredientReferences": [],
            "summary": null,
            "text": "Stir together pudding, milk as directed on package.  While cooking add chocolate chips until they are melted.  Set aside.",
            "title": ""
        },
        {
            "id": "d2f470de-2917-4fa0-8cf3-19516627a512",
            "ingredientReferences": [],
            "summary": null,
            "text": "In med bowl beat cream cheese (softened), powdered  sugar, peppermint extract until smooth.  Gently fold 2 cups of cool whip.",
            "title": ""
        },
        {
            "id": "8464c198-4121-4b20-b4fc-c34289278589",
            "ingredientReferences": [],
            "summary": null,
            "text": "Spread cream mixture in the shell.  Top with chocolate mixture.  Refrigerate at least 2 hours.",
            "title": ""
        }
