    "recipeInstructions": [
        {
            "id": "c59d3b34-3518-4953-a448-8291d302d596",
            "ingredientReferences": [],
            "summary": null,
            "text": "Cook pasta in a large pot of boiling water until done. Drain.\n\n",
            "title": ""
        },
        {
            "id": "8bbbaf43-cca0-4995-80cb-a991707182c3",
            "ingredientReferences": [],
            "summary": null,
            "text": "Meanwhile, combine oil, peanuts and onions in a small skillet. Saute over low heat until tender. Add ginger; cook and stir for 1 to 2 minutes. Mix in peanut butter, soy sauce, water, vinegar, sugar, and red pepper flakes. Mix thoroughly until peanut butter in melted and it's thick.  Remove from heat.",
            "title": ""
        },
        {
            "id": "20881ffa-572c-4eb0-8df3-468cb346f6f9",
            "ingredientReferences": [],
            "summary": null,
            "text": "Toss noodles with sauce, and serve.",
            "title": ""
        }
    ],
