    "recipeInstructions": [
        {
            "id": "53cb940c-65de-4155-943c-9cab38144dcd",
            "title": "",
            "text": "Preheat a soup pot over medium-high heat with the olive oil.",
            "ingredientReferences": []
        },
        {
            "id": "439265d4-140b-4fce-86f9-f58bb93ae6ab",
            "title": "",
            "text": "Added the sliced chorizo and cook for 2 minutes",
            "ingredientReferences": []
        },
        {
            "id": "662b55ca-c1be-4292-a5d6-195c52a775fc",
            "title": "",
            "text": "Add the onions, carrot and garlic.  Season with salt and pepper and cook, stirring frequently, for 5 minutes.",
            "ingredientReferences": []
        },
        {
            "id": "81117e38-f87d-47a4-9248-4c447d04d08e",
            "title": "",
            "text": "Add the white wine and cook for 3 minutes.",
            "ingredientReferences": []
        },
        {
            "id": "a37af833-bedc-43cb-a075-c579c4dce954",
            "title": "",
            "text": "Add the fire-roasted tomatoes, chicken stock and roasted red-peppers.  Bring up to a simmer and cook for 5 minutes.",
            "ingredientReferences": []
        },
        {
            "id": "a8812417-f69e-4ba8-8090-f400ccddeca7",
            "title": "",
            "text": "You want the soup to be a gentle simmer before you add the fish so, if necessary, turn the heat down a little.",
            "ingredientReferences": []
        },
        {
            "id": "42fe50ad-f9e9-4035-b22c-7a1d63456186",
            "title": "",
            "text": "Add the cod.  Gently simmer for 3-4 minutes, or until the fish is cooked through.",
            "ingredientReferences": []
        },
        {
            "id": "23f9e1ac-09e5-4b9b-a761-c5d5a8ec7b93",
            "title": "",
            "text": "Finish the soup with parsley, taste for seasoning, and serve",
            "ingredientReferences": []
        }
    ],
