    "recipeInstructions": [
        {
            "id": "5dcdce07-c839-4e48-a335-ab890a9ae970",
            "ingredientReferences": [],
            "summary": null,
            "text": "Add peper, cumin, dill weed, onion and adobo to rice more less in equal proportions.  Add a quarter clove of chopped garlic.  Cook in rice cooker, using chicken broth instead of water.",
            "title": ""
        },
        {
            "id": "795fe7e8-5212-48ec-8119-e9f5c43280a4",
            "ingredientReferences": [],
            "summary": null,
            "text": "Defrost chicken and cut chicken to small chunks.",
            "title": ""
        },
        {
            "id": "15b99825-aa73-4433-8a26-eb3a1c58e04d",
            "ingredientReferences": [],
            "summary": null,
            "text": "Wait about 10 minutes while rice is cooking.",
            "title": ""
        },
        {
            "id": "9614baf7-cefe-4efd-95f4-ef1759df6fae",
            "ingredientReferences": [],
            "summary": null,
            "text": "In Medium pan add teaspoon olive oil.  Add a 1/4 clove garlic and dried onion.  Bring heat up to medium low and let it go a minute or so, pushing the onions and garlic around.  Add enough chicken broth to cover bottom of pan.",
            "title": ""
        },
        {
            "id": "d5e0f3f7-b5c1-4d60-ac57-6d4350795df8",
            "ingredientReferences": [],
            "summary": null,
            "text": "Add chicken, pepper, cumin, dill weed and adobo.  set temperature to low and cook chicken, add more broth if needed.  The chicken pieces should be small so this shouldn't take all that long even on low.",
            "title": ""
        },
        {
            "id": "7bf1aef4-10e1-4592-ae2a-dc42e25ceb19",
            "ingredientReferences": [],
            "summary": null,
            "text": "Combine rice and chicken and serve.",
            "title": ""
        }
    ],
